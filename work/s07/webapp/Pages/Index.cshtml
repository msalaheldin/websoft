@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
    
<div class="text-center">
    <h1 class="display-4">Welcome websoft</h1>
</div>


<table style="width:100%" >
    <tr>
        <th>Number</th>
        <th>Balance</th>
        <th>Label</th>
        <th>Owner</th>
    </tr>


@foreach (var account in Model.Accounts)
{
<tr>
    <td>@account.Number</td>
    <td>@account.Balance</td>
    <td>@account.Label</td>
    <td>@account.Owner</td>
</tr>
}

</table>